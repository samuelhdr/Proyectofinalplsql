Create table vehiculos (
vehiculo int not null,
marcha varchar(11),
modelo varchar(11),
ano date,
estado varchar(6),
fechaentra timestamp
);
create table vehiculos
(
vehiculoid int not null,
marca varchar(11),
medole varchar(11),
ano date,
estado varchar(10),
fechaen timestamp
);

ALTER TABLE vehiculos
ADD CONSTRAINT vehiculo_pk PRIMARY KEY (vehiculoid);

create table registro(
registroid int not null,
fechaentra timestamp,
fechasalida timestamp,
clienteid int not null,
mecanicoid int,
chequeoid int 
);

alter table registro
add constraint registro_pk primary key (registroid);


create table chequeo(
chequeoid int not null,
pieza varchar(30),
des varchar(15),
fechaini timestamp,
fechafin timestamp,
mecanicoid int,
area varchar(20)

);
alter table chequeo
add constraint chequeoid_pk primary key (chequeoid);

create table costo(
costoid int not null,
garantia varchar(12),
precio int,
clienteid int
);
alter table costo
add constraint costoid_pk primary key (costoid);
create table cliente(
cleinteid int not null,
nombre varchar(25),
apellido varchar(21),
vehiculoid int 

alter table cliente 
add constraint cleinteid_pk primary key (cleinteid);

create sequence seq_clienteid
increment by 10;
insert into cliente values (seq_clienteid.nextval,'samuel','sencion','25');
desc cliente;

select * from cliente;
desc costo;
desc vehiculo;
desc cliente;
